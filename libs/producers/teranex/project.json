{
    "root": "libs/producers/teranex",
    "sourceRoot": "libs/producers/teranex/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/producers/teranex",
                "tsConfig": "libs/producers/teranex/tsconfig.lib.json",
                "packageJson": "libs/producers/teranex/package.json",
                "main": "libs/producers/teranex/src/index.ts",
                "assets": ["libs/producers/teranex/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/producers/teranex/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/producers/teranex"],
            "options": {
                "jestConfig": "libs/producers/teranex/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
