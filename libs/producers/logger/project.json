{
    "root": "libs/producers/logger",
    "sourceRoot": "libs/producers/logger/src",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/producers/logger",
                "main": "libs/producers/logger/src/index.ts",
                "tsConfig": "libs/producers/logger/tsconfig.lib.json",
                "assets": ["libs/producers/logger/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/producers/logger/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/producers/logger"],
            "options": {
                "jestConfig": "libs/producers/logger/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
