{
    "root": "libs/watcher",
    "sourceRoot": "libs/watcher/src",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/watcher",
                "main": "libs/watcher/src/index.ts",
                "tsConfig": "libs/watcher/tsconfig.lib.json",
                "assets": ["libs/watcher/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/watcher/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/watcher"],
            "options": {
                "jestConfig": "libs/watcher/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
