{
    "root": "libs/main-service-modules/decklink-live-monitor",
    "sourceRoot": "libs/main-service-modules/decklink-live-monitor/src",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/main-service-modules/decklink-live-monitor",
                "main": "libs/main-service-modules/decklink-live-monitor/src/index.ts",
                "tsConfig": "libs/main-service-modules/decklink-live-monitor/tsconfig.lib.json",
                "assets": ["libs/main-service-modules/decklink-live-monitor/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/main-service-modules/decklink-live-monitor/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/main-service-modules/decklink-live-monitor"],
            "options": {
                "jestConfig": "libs/main-service-modules/decklink-live-monitor/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
