{
    "root": "apps/main-service",
    "sourceRoot": "apps/main-service/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/node:webpack",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/main-service",
                "main": "apps/main-service/src/main.ts",
                "tsConfig": "apps/main-service/tsconfig.app.json",
                "assets": ["apps/main-service/src/assets"]
            },
            "configurations": {
                "production": {
                    "externalDependencies": "none",
                    "optimization": true,
                    "extractLicenses": true,
                    "inspect": false,
                    "fileReplacements": [
                        {
                            "replace": "apps/main-service/src/environments/environment.ts",
                            "with": "apps/main-service/src/environments/environment.prod.ts"
                        }
                    ]
                }
            }
        },
        "serve": {
            "executor": "@nrwl/node:node",
            "options": {
                "buildTarget": "nxt-realtime-main-service-server:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/main-service/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/main-service"],
            "options": {
                "jestConfig": "apps/main-service/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
